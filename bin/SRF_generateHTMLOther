open (IN,"$nameRNAfold");
my %score;
my $idG = '';
my $contig;
my $contig2;
my @dates;
my %alignments = ();
#Extract score and structure of RNAfold output
while (<IN>) {
        if(!/^#/){
                chomp($_);
                if(/^>/){
                	$contig = substr($_,1);
			$dates[0]= $contig; #nombre completo
			my @rna = split ("-",$contig);
			pop(@rna);
			pop(@rna);
			$contig2 = join("-",@rna);
                }
		elsif(/^[AUCG]/i){
			chomp($_);
			$dates[1] = $_;			
		}
                elsif(/^\./ || /^\(/){
			chomp($_);
			$dates[2] = $_;
                        my @sec= split (/\(/,$_);
                        my $score = pop(@sec);
                        chop($score);
			$dates[3]= $score;
                        $score{$contig2} = [@dates];
			@dates = '';
        	}		
	}
}

