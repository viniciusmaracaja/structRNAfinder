								pop(@align); pop(@align);
								chomp(@align);
							}					
							my $complete_align = join("\n\n",@align);
							$complete_align =~ s/\n/\<br\>/ig;
							$complete_align =~ s/\s/\&nbsp\;/ig;
							if($alignments{$id[0]}) {
								if($fields[3] < $alignments{$id[0]}[0] ){
									$alignments{$ids}[1] = $complete_align;
									$alignments{$ids}[0] = $fields[3];
								}	
							} 
							else{
								$alignments{$ids}[1] = $complete_align;
								$alignments{$ids}[0] = $fields[3];
							}
						}
					}
				}
			}
		}
		close(IOUT);
	}
}#end all report

$/ = "\n";

